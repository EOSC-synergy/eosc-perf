{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "type": "object",
  "properties": {
    "arguments": {
      "const": "Total Time - 4K, 16 Rays Per Pixel"
    },
    "machine": {
      "type": "object",
      "properties": {
        "cpu": {
          "type": "object",
          "properties": {
            "arch": {
              "type": "string",
              "suggestToUser": true
            },
            "bits": {
              "type": "number"
            },
            "brand_raw": {
              "type": "string"
            },
            "count": {
              "type": "number",
              "suggestToUser": true
            },
            "hz_actual_friendly": {
              "type": "string"
            }
          },
          "required": [
            "count"
          ]
        }
      }
    },
    "result": {
      "description": "The execution results",
      "type": "object",
      "properties": {
        "all_results": {
          "type": "string"
        },
        "score": {
          "type": "number",
          "suggestToUser": true
        }
      },
      "required": [
        "all_results",
        "score"
      ]
    },
    "test": {
      "const": "pts/c-ray-1.2.0"
    },
    "units": {
      "const": "Seconds"
    }
  },
  "required": [
    "arguments",
    "machine",
    "result",
    "test",
    "units"
  ]
}